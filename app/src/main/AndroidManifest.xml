<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name=".Application"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:enableOnBackInvokedCallback="true"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Meku">
        <activity
            android:name=".page.subject_management_page.SubjectDetailActivity"
            android:exported="false" />
        <activity
            android:name=".page.subject_management_page.SubjectManagementPageActivity"
            android:exported="false" />
        <activity
            android:name=".page.account_management_page.AccountDetailActivity"
            android:exported="false" />
        <activity
            android:name=".page.account_management_page.AccountManagementPageActivity"
            android:exported="false" />
        <activity
            android:name=".page.test_page.MainActivity2"
            android:exported="true"
            android:theme="@style/Theme.Meku" />
        <activity
            android:name=".page.initial_page.MainActivity"
            android:exported="true"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".page.test_page.TestActivity"
            android:exported="true"
            android:theme="@style/Theme.Meku" />
    </application>

</manifest>